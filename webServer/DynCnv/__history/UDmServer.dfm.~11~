object DMServer: TDMServer
  OldCreateOrder = False
  Encoding = esASCII
  Height = 150
  Width = 215
  object api: TDWServerEvents
    IgnoreInvalidParams = False
    Events = <
      item
        Routes = [crAll]
        DWParams = <>
        JsonMode = jmPureJSON
        Name = 'ver'
        OnReplyEventByType = apiEventsverReplyEventByType
      end
      item
        Routes = [crGet]
        DWParams = <
          item
            TypeObject = toParam
            ObjectDirection = odIN
            ObjectValue = ovInteger
            ParamName = 'code_unit'
            Encoded = True
            DefaultValue = '1'
          end>
        JsonMode = jmPureJSON
        Name = 'dyncnv'
        OnReplyEventByType = apiEventsdyncnvReplyEventByType
      end>
    Left = 16
    Top = 16
  end
end
